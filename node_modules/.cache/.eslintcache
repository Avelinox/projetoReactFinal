{id,Nome_do_produto,Preco,Tamanho,Cor} = resposta.data;\r\n\r\n    configProduto(produtos.map(produto =>{\r\n      return produto.id ===id ? {...resposta.data}: produto\r\n    }))\r\n\r\n  }\r\n\r\n  //codigo para eliminar algo cadastrado\r\n\r\n  const deletarProd = async (id)=>{\r\n    await  api.delete(`/produtos/${id}`);\r\n\r\n      const nvProduto = produtos.filter((produto)=>{\r\n        return produto.id !== id;\r\n      });\r\n      configProduto(nvProduto);\r\n  }\r\n\r\n\r\n\r\n\r\n//criando efeito de como os dados vÃ£o ser apresentados\r\n  useEffect(()=>{\r\n\r\n    // const visualizarProdutos =JSON.parse(localStorage.getItem(LOCAL_STORAGE_KEY,JSON));\r\n\r\n    // if(visualizarProdutos){\r\n    //   configProduto(visualizarProdutos)\r\n    // }\r\n\r\n    const getProdutos = async ()=>{\r\n      const todosProdutos =  await recuperarProduto();\r\n      if(todosProdutos){\r\n        configProduto(todosProdutos);\r\n      }\r\n    }\r\n    getProdutos();\r\n  },[])\r\n\r\n  useEffect(()=>{\r\n    localStorage.setItem(LOCAL_STORAGE_KEY, JSON.stringify(produtos));\r\n\r\n  },[produtos])\r\n  return (\r\n    <div className = \"ui container\">\r\n\r\n    <Router>\r\n\r\n      <Header />\r\n\r\n      <Switch>\r\n      {/* <Route path =\"/\" exact component ={()=><ListaProdutos produtos={produtos} produtoId ={deletarProd}/>}/> */}\r\n\r\n        <Route path =\"/\" exact render={(props)=>(<ListaProdutos {...props} produtos={produtos} produtoId ={deletarProd}/>)} \r\n        />\r\n        \r\n        <Route path =\"/add\" render={(props)=>(<AdicionarProduto {...props} novoProduto ={ novoProduto}/>)}\r\n        />\r\n\r\n      <Route path =\"/edit\" render={(props)=>(<EditProduto {...props} updateProduto ={ updateProduto}/>)}\r\n        />\r\n        <Route path=\"/produto/:id\" component={ProdutoInfo}/>\r\n       \r\n      </Switch>\r\n      \r\n      {/* <AdicionarProduto novoProduto ={ novoProduto}/> */}\r\n      {/* Adicionando minha variavel na lista dos produtos */}\r\n      {/* <ListaProdutos produtos={produtos} produtoId ={deletarProd}/> */}\r\n      \r\n    </Router>\r\n\r\n\r\n     \r\n    </div>\r\n\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\ADM\\Desktop\\Original\\projetoFakeApi-reactJS\\src\\componentes\\header.js",[],"C:\\Users\\ADM\\Desktop\\Original\\projetoFakeApi-reactJS\\src\\componentes\\Editproduto.js",[],"C:\\Users\\ADM\\Desktop\\Original\\projetoFakeApi-reactJS\\src\\componentes\\ListProduto.js",[],"C:\\Users\\ADM\\Desktop\\Original\\projetoFakeApi-reactJS\\src\\componentes\\Addproduto.js",[],"C:\\Users\\ADM\\Desktop\\Original\\projetoFakeApi-reactJS\\src\\componentes\\ProdutoInfo.js",[],"C:\\Users\\ADM\\Desktop\\Original\\projetoFakeApi-reactJS\\src\\api\\produtos.js",[],"C:\\Users\\ADM\\Desktop\\Original\\projetoFakeApi-reactJS\\src\\componentes\\ProdutCard.js",[],{"ruleId":"43","severity":1,"message":"44","line":43,"column":14,"nodeType":"45","messageId":"46","endLine":43,"endColumn":29},{"ruleId":"43","severity":1,"message":"47","line":43,"column":30,"nodeType":"45","messageId":"46","endLine":43,"endColumn":35},{"ruleId":"43","severity":1,"message":"48","line":43,"column":36,"nodeType":"45","messageId":"46","endLine":43,"endColumn":43},{"ruleId":"43","severity":1,"message":"49","line":43,"column":44,"nodeType":"45","messageId":"46","endLine":43,"endColumn":47},"no-unused-vars","'Nome_do_produto' is assigned a value but never used.","Identifier","unusedVar","'Preco' is assigned a value but never used.","'Tamanho' is assigned a value but never used.","'Cor' is assigned a value but never used."]